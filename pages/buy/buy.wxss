/* 列表部分 */
.xiaowei_list{
    color:rgb(46, 51, 62);
    line-height:48rpx;
    box-sizing:border-box;
    border-radius: 15rpx;
    padding: 22rpx 24rpx 24rpx;
    background-color: #fff;
    margin-top: 20rpx;
}
.xiaowei_list .xiaowei_1_public{
    display: flex;
    justify-content: space-between;
}
.xiaowei_list .xiaowei_1{
    color: rgb(46, 51, 62);
    font-size: 32rpx;
    font-weight: 500;
}
.xiaowei_list .left{
    overflow: hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    width: 60vw;
}
.xiaowei_list .right text:nth-child(1){
    color: rgb(255, 67, 97);
    font-size: 24rpx;
    font-weight: 400;
}

.xiaowei_list .right text:nth-child(2){
    color: rgb(149, 154, 165);
    font-size: 24rpx;
    font-weight: 400;
}
.xiaowei_list .xiaowei_2 view:nth-child(1){
    color:rgb(95, 102, 114);
    font-size: 24rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    width: 70vw;
}
.xiaowei_list .xiaowei_2 view:nth-child(2){
    font-size: 24rpx;
}
.xiaowei_3{
    display: flex;
    align-items: center;
}
.xiaowei_3 image{
    width: 75rpx;
    height: 30rpx;
}
